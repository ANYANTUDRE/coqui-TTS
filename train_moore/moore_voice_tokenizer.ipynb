    "class VoiceMooreTextPreprocessor:\n",
    "\n",
    "    def preprocess_batch(self, texts: List[str]) -> List[str]:\n",
    "        return [self.preprocess(text) for text in texts]\n",
    "\n",
    "    def preprocess(self, text: str) -> str:\n",
    "        if type(text) == NoneType:\n",
    "            text = str(text)\n",
    "        text = text.lower()\n",
    "        \n",
    "        return text"
   ]
  },
